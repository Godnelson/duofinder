{
  "name": "duofinder",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",

    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:migrate": "pnpm --filter @duo/api prisma:migrate",
    "db:seed": "pnpm --filter @duo/api prisma:seed"
  },
  "devDependencies": {
    "turbo": "^2.2.3"
  }
}
